{
    "compilerOptions": {
        "target": "ES6",
        "allowJs": true,
        "module": "CommonJS",
        "esModuleInterop": true,
        "strict": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "baseUrl": "src",
        "outDir": "dist"
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}